  "bugs": {
    "url": "https://github.com/akinncar/react-native-mask-text/issues"
  },
  "homepage": "https://github.com/akinncar/react-native-mask-text#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
