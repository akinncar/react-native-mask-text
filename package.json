{
  "name": "react-native-mask-text",
  "version": "0.0.0",
  "description": "A React Native and Expo library to mask text",
  "keywords": [
    "react",
    "react-native",
    "input",
    "mask",
    "expo",
    "text-input"
  ],
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
